{
  "policies": [
    {
      "name": "my-stripe-webhook-verification-inbound-policy",
      "policyType": "stripe-webhook-verification-inbound",
      "handler": {
        "export": "StripeWebhookVerificationInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "tolerance": 300
        }
      }
    }
  ],
  "corsPolicies": [
    {
      "name": "custom-cors",
      "allowedOrigins": [
        "http://localhost:3000",
        "https://freshicons.co"
      ],
      "allowedHeaders": [
        "Content-Type",
        "Authorization",
        "Origin"
      ],
      "allowedMethods": [
        "GET",
        "POST",
        "OPTIONS"
      ],
      "exposeHeaders": [],
      "maxAge": 600,
      "allowCredentials": false
    }
  ]
}